{
    "entryPoints": ["./src"],
    "entryPointStrategy": "expand",
    "out": "./docs",
    "includeVersion": true,
    "exclude": [
        "**/*.test.*",
        "**/*.spec.*",
        "node_modules/**"
    ],
    "excludeExternals": true,
    "excludePrivate": true,
    "theme": "markdown",
    "tsconfig": "./tsconfig.json",
    "hideGenerator": true,
    "readme": "none",
    "plugin": ["typedoc-plugin-markdown"]
}